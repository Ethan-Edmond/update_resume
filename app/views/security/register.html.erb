<ul>
  <% @users.each do |user| %>
  <li>
    <%= user.username %>
  </li>
  <% end %>
</ul>

<%= form_with model: @user do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.submit %>
<% end %>

